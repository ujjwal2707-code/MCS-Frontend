{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "typeRoots": ["node_modules/@types", "src/types"],
    "types": ["jest"],
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@navigation/*": ["src/navigation/*"],
      "@components/*": ["src/components/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"],
      "@context/*":["src/context/*"],
      "@constants/*":["src/constants/*"]
    }
  }
}
